{
    "name": "mattlibera/cron",
    "description": "Job scheduling for Laravel - Matt Libera's fork",
    "keywords": ["laravel", "cron", "cronjob", "cronmanager"],
    "homepage": "https://github.com/mattlibera/cron",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Libera",
            "email": "matthew@liberawebdesign.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "mtdowling/cron-expression": "1.*"
    }, 
    "require-dev": {
        "illuminate/support": "4.0.x|~5.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Mattlibera\\Cron": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mattlibera\\Cron\\Laravel5ServiceProvider"
            ]
        }
    }
}
